{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Assistant Core",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ai_assistant_core/main.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal",
            "pythonArgs": [
                "-Xfrozen_modules=off"
            ]
        }
    ]
}
